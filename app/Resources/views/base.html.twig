<!DOCTYPE html>
<html lang="FR,fr">
<head>

    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <title>{% block title %}Benin agriculture!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('front/css/main.css') }}">
        <!-- Font-icon css-->
        <link rel="stylesheet" type="text/css"
        href="http://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    {% endblock %}
    <!--link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/-->
    <link rel="icon" type="image/x-icon" href="{{ asset('icone.ico') }}"/>
</head>
<body class="app sidebar-mini rtl">


{% block header %}
    {% include 'block/nav.html.twig' %}
{% endblock %}
<div class="app-sidebar__overlay" data-toggle="sidebar"></div>
<aside class="app-sidebar">

    {% block userDeatil %}
        {% include 'block/userDetail.html.twig' %}
    {% endblock %}

    {% block sidebar %}
        {% include 'block/sidebar.html.twig' %}
    {% endblock %}
</aside>

<main class="app-content">
    <div class="app-title">
        <div>
            {% block titlePage %}
                {% include 'block/titrePage.html.twig' %}
            {% endblock %}
        </div>
        {% block breadcrumb %}
            {% include 'block/breadcrumb.html.twig' %}
        {% endblock %}
    </div>

        {% block content %}
            {% include 'block/content.html.twig' %}
        {% endblock %}

</main>


{% block javascripts %}
    <!-- Essential javascripts for application to work-->
    <script src="{{ asset('front/js/jquery-3.2.1.min.js') }}"></script>
    <script src="{{ asset('front/js/popper.min.js') }}"></script>
    <script src="{{ asset('front/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('front/js/main.js') }}"></script>
    <!-- The javascript plugin to display page loading on top-->
    <script src="{{ asset('front/js/plugins/pace.min.js') }}"></script>
    <!-- Page specific javascripts-->

    <script type="text/javascript" src="{{ asset('front/js/plugins/bootstrap-datepicker.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('front/js/plugins/select2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('front/js/plugins/bootstrap-datepicker.min.js') }}"></script>

    <!-- Google analytics script-->
    <script type="text/javascript">
        if (document.location.hostname == 'pratikborsadiya.in') {
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                            (i[r].q = i[r].q || []).push(arguments)
                        }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                        m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '../../www.google-analytics.com/analytics.js', 'ga');
            ga('create', 'UA-72504830-1', 'auto');
            ga('send', 'pageview');
        }
     
      $('.demoDate').datepicker({
        format: "dd/mm/yyyy",
        autoclose: true,
        todayHighlight: true,
        drop: 'down'
      });
      

    </script>
{% endblock %}
</body>
</html>
